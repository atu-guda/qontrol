   <param name="long_descr">Test TDiffIn.

Test fun:
y=7x^2+3x-9;
dy/dx=14x+3;
d^2y/dx^2=14;

near x=2;</param>
   <param name="script">function test_tdiffin()
{
  var v = main_s.diff.getDataD( "d_rc", 0.0 );
  print( "d_rc = ", v, "\n" );
  if( ! isNear( v, 31, 0.02 ) ) {
    print( "Bad Value d_rc\n" );
    return 1;
  }

  v = main_s.diff.getDataD( "d_rl", 0.0 );
  print( "d_rl = ", v, "\n" );
  if( ! isNear( v, 31, 0.01 ) ) {
    print( "Bad Value d_rl\n" );
    return 1;
  }

  v = main_s.diff.getDataD( "d_cl", 0.0 );
  print( "d_cl = ", v, "\n" );
  if( ! isNear( v, 31, 0.02 ) ) {
    print( "Bad Value d_cl\n" );
    return 1;
  }

  v = main_s.diff.getDataD( "d2", 0.0 );
  print( "d2 = ", v, "\n" );
  if( ! isNear( v, 14, 0.0001 ) ) {
    print( "Bad Value d2\n" );
    return 1;
  }


  return 0;
}


